{
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 28
    },
    "configurePresets": [
        {
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "BOARD": "@BOARD@",
                "NO_BUILD_TYPE_WARNING": "On"
            },
            "environment": {
                "PATH": "@PATH@",
                "ZEPHYR_BASE": "@ZEPHYR_BASE@",
                "ZEPHYR_SDK_INSTALL_DIR": "@ZEPHYR_SDK_INSTALL_DIR@"
            },
            "generator": "@CMAKE_GENERATOR@",
            "name": "Zephyr"
        }
    ],
    "version": 10
}
