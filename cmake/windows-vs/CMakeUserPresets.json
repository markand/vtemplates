{
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 28,
        "patch": 0
    },
    "configurePresets": [
        {
            "architecture": {
                "strategy": "external",
                "value": "x64"
            },
            "cacheVariables": {
                "CMAKE_CXX_COMPILER": "cl.exe",
                "CMAKE_C_COMPILER": "cl.exe"
            },
            "hidden": true,
            "name": "msvc-x86_64",
            "toolset": {
                "strategy": "external",
                "value": "host=x64"
            }
        },
        {
            "architecture": {
                "strategy": "external",
                "value": "x86"
            },
            "cacheVariables": {
                "CMAKE_CXX_COMPILER": "cl.exe",
                "CMAKE_C_COMPILER": "cl.exe"
            },
            "hidden": true,
            "name": "msvc-x86",
            "toolset": {
                "strategy": "external",
                "value": "host=x86"
            }
        },
        {
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            },
            "generator": "Ninja",
            "inherits": "msvc-x86_64",
            "name": "Debug (x86-64)"
        },
        {
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            },
            "generator": "Ninja",
            "inherits": "msvc-x86_64",
            "name": "Release (x86-64)"
        },
        {
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            },
            "generator": "Ninja",
            "inherits": "msvc-x86",
            "name": "Debug (x86)"
        },
        {
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            },
            "generator": "Ninja",
            "inherits": "msvc-x86",
            "name": "Release (x86)"
        }
    ],
    "version": 10
}
